{
    "name": "raydotnl/laravel-exact-online",
    "description": "Laravel Facade wrapper for Exact Online PHP API",
    "require": {
        "picqer/exact-php-client": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua de Gier",
            "email": "info@pendo.nl"
        },
        {
            "name": "Sjoerd Rutten",
            "email": "sjoerd@ray.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Raydotnl\\LaravelExactOnline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raydotnl\\LaravelExactOnline\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Raydotnl\\LaravelExactOnline\\Providers\\LaravelExactOnlineServiceProvider"
            ],
            "aliases": {
                "ExactOnline": "Raydotnl\\LaravelExactOnline\\LaravelExactOnlineFacade"
            }
        }
    }
}
